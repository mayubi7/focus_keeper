{
  "name": "focus keeper",
  "version": "1.0.0",
  "manifest_version": 3,
  "permissions": [
    "storage",
    "alarms",
    "tabs"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Focus Keeper"
  }
}


